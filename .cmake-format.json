{
  "format": {
    "disable": false,
    "line_width": 120,
    "tab_size": 4,
    "max_subgroups_hwrap": 2,
    "max_pargs_hwrap": 4,
    "dangle_parens": false,
    "min_prefix_chars": 4,
    "max_prefix_chars": 10,
    "max_lines_hwrap": 2,
    "line_ending": "auto",
    "command_case": "canonical",
    "keyword_case": "upper",
    "enable_sort": true,
    "autosort": true
  }
}
