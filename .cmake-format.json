{
  "additional_commands": {
    "td_add_library": {
      "pargs": 1,
      "kwargs": {
        "SOURCES": "*",
        "PUBLIC_DEPS": "*",
        "PRIVATE_DEPS": "*"
      }
    },
    "td_add_application": {
      "pargs": 1,
      "kwargs": {
        "SOURCES": "*",
        "DEPS": "*"
      }
    },
    "td_add_console_application": {
      "pargs": 1,
      "kwargs": {
        "SOURCES": "*",
        "DEPS": "*"
      }
    },
    "td_add_test": {
      "pargs": 1,
      "kwargs": {
        "SOURCES": "*",
        "DEPS": "*",
        "TEST_ARGS": "*"
      }
    },
    "exec": {
      "pargs": 0,
      "kwargs": {
        "COMMAND": "*",
        "OUTPUT_FILE": "1"
      }
    },
    "vcpkg_from_github": {
      "pargs": 0,
      "kwargs": {
        "OUT_SOURCE_PATH": "1",
        "REPO": "1",
        "REF": "1",
        "SHA512": "1",
        "HEAD_REF": "1",
        "PATCHES": "*",
        "GITHUB_HOST": "1",
        "AUTHORIZATION_TOKEN": "1",
        "FILE_DISAMBIGUATOR": "1"
      }
    },
    "FetchContent_Declare": {
      "pargs": 1,
      "kwargs": {
        "URL": "1",
        "URL_HASH": "1",
        "DOWNLOAD_EXTRACT_TIMESTAMP": "1",
        "GIT_REPOSITORY": "1",
        "GIT_TAG": "1",
        "GIT_SHALLOW": "1",
        "PREFIX": "1",
        "SOURCE_SUBDIR": "1",
        "CMAKE_ARGS": "1",
        "BUILD_COMMAND": "*",
        "TEST_COMMAND": "*",
        "INSTALL_COMMAND": "*"
      }
    },
    "execute_process": {
      "pargs": "*",
      "kwargs": {
        "COMMAND": "*",
        "OUTPUT_FILE": "1",
        "COMMAND_ECHO": "1",
        "COMMAND_ERROR_IS_FATAL": "1"
      }
    },
    "vcpkg_cmake_configure": {
      "pargs": 0,
      "kwargs": {
        "SOURCE_PATH": "1",
        "DISABLE_PARALLEL_CONFIGURE": "0",
        "NO_CHARSET_FLAG": "0",
        "WINDOWS_USE_MSBUILD": "0",
        "GENERATOR": "1",
        "LOGFILE_BASE": "1",
        "OPTIONS": "*",
        "OPTIONS_RELEASE": "*",
        "OPTIONS_DEBUG": "*",
        "MAYBE_UNUSED_VARIABLES": "*"
      }
    }
  },
  "format": {
    "disable": false,
    "line_width": 120,
    "tab_size": 4,
    "max_subgroups_hwrap": 2,
    "max_pargs_hwrap": 4,
    "dangle_parens": false,
    "min_prefix_chars": 4,
    "max_prefix_chars": 10,
    "max_lines_hwrap": 2,
    "line_ending": "auto",
    "command_case": "canonical",
    "keyword_case": "upper",
    "enable_sort": true,
    "autosort": true
  }
}
