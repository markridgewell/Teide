{
  "format": {
    "disable": false,
    "line_width": 120,
    "tab_size": 4,
    "max_subgroups_hwrap": 2,
    "max_pargs_hwrap": 4,
    "dangle_parens": false,
    "min_prefix_chars": 4,
    "max_prefix_chars": 10,
    "max_lines_hwrap": 2,
    "line_ending": "auto",
    "command_case": "canonical",
    "keyword_case": "upper",
    "_help_enable_sort": [
      "If true, the argument lists which are known to be sortable",
      "will be sorted lexicographicall"
    ],
    "enable_sort": true,
    "_help_autosort": [
      "If true, the parsers may infer whether or not an argument",
      "list is sortable (without annotation)."
    ],
    "autosort": true
  }
}
