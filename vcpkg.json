{
  "name": "teide",
  "version": "0.0.0",
  "builtin-baseline": "1de2026f28ead93ff1773e6e680387643e914ea1",
  "dependencies": [
    "fmt",
    "function2",
    {
      "name": "glslang",
      "features": [
        "rtti"
      ]
    },
    {
      "name": "sdl2",
      "features": [
        "vulkan"
      ]
    },
    "sdl2-image",
    "spdlog",
    "taskflow",
    "vulkan",
    "vulkan-headers",
    "vulkan-memory-allocator-hpp",
    "stdexec"
  ],
  "features": {
    "benchmarks": {
      "description": "Build benchmarks",
      "dependencies": [
        "benchmark"
      ]
    },
    "examples": {
      "description": "Build examples",
      "dependencies": [
        "assimp"
      ]
    },
    "swiftshader": {
      "description": "Build with Swiftshader support",
      "dependencies": [
        {
          "name": "llvm",
          "default-features": false,
          "features": [
            "enable-rtti"
          ]
        },
        "swiftshader"
      ]
    },
    "tests": {
      "description": "Build tests",
      "dependencies": [
        "gtest",
        {
          "name": "stackwalker",
          "platform": "windows & !uwp"
        }
      ]
    }
  }
}
